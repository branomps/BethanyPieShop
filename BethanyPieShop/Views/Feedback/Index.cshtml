@model Feedback


<form asp-action="Index" method="post">

    <div class="form-group">
        <label asp-for="Name" class="col-md-2 col-form-label"></label>
        <div class="col-md-5">
            <input asp-for="Name" class="form-control" />
        </div>
    </div>
    <div class="form-group">
        <label asp-for="Email" class="col-md-2 col-form-label"></label>
        <div class="col-md-5">
            <input asp-for="Email" class="form-control" />
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Message" class="col-md-2 col-form-label"></label>
        <div class="col-md-5">
            <textarea asp-for="Message" class="form-control"></textarea>
        </div>
    </div>

    <div class="form-group">
        <label asp-for="ContactMe" class="col-md-2 col-form-label"></label>
        <div class="col-md-5">
            <input type="checkbox" asp-for="ContactMe" class="form-control" />
        </div>
    </div>

    <div class="form-group">
        <div class=" col-md-5">
            <input type="submit" class="btn btn-primary" value="Send feedback" />
        </div>
    </div>

</form> 